#include<iostream>
#include<vector>
#include<deque>
#include<algorithm>
using namespace std;
#define optimize ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
typedef long long ll;
#define endl "\n"

int main(){
 optimize;
 deque<int>dq;
 dq.push_front(2);
 dq.push_front(3);
 dq.push_back(4);
 dq.push_front(5);
 dq.push_back(6);
 dq.push_back(7);
 dq.push_front(8);
 
 cout << dq.front() << " " << dq.back() << endl; // 8, 7

 dq.pop_back();
 cout << dq.front() << " " << dq.back() << endl; // 8 6
 
 dq.pop_front();
 cout << dq.front() << " " << dq.back() << endl; // 5 6

 
 return 0;

}
